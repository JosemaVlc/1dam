<Window x:Class="controlesC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:controlesC"
        mc:Ignorable="d"
        Title="Examen: Jose Manuel Moreno Bolivar" Height="450" Width="800" WindowStartupLocation="CenterScreen" Topmost="True">
    <UniformGrid Rows="3" 
                 Columns="6"
                 x:Name="contenedorBotones">        
    </UniformGrid>
    <Window.Resources>
        <Style TargetType="Button" x:Key="botonUniformGrid">
            <Setter Property="Background" Value="DarkGoldenrod"/>
            <Setter Property="Foreground" Value="White"/>
        </Style>
    </Window.Resources>
</Window>
