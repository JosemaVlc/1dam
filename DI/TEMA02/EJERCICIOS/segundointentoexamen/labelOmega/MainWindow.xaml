<Window x:Class="labelOmega.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:labelOmega"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <UniformGrid Columns="5" Rows="5" x:Name="Patatasfritas">        
    </UniformGrid>
    <Window.Resources>
        <Style TargetType="Label" x:Key="colorespatata">
            <Setter Property="Background"  Value="Indigo"/>
            <Setter Property="Foreground" Value="Fuchsia"/>
        </Style>
    </Window.Resources>
</Window>
